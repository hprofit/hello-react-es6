{
  "author": "scott preston",
  "name": "react-starter",
  "description": "simple project to highlight react with routing and redux.",
  "version": "0.0.1",
  "dependencies": {
    "browserify": "^13.0.1",
    "http-server": "^0.9.0",
    "jquery": "^3.0.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "redux": "^3.5.2"
  },
  "scripts": {
    "build": "babel src -d lib",
    "ify": "browserify lib/*.js -o dist/app.js",
    "pack": "webpack ./lib/index.js ./dist/app.bundle.js",
    "prestart": "npm run build && npm run pack && npm run test-coverage",
    "start": "http-server -p 5000",
    "test": "babel-node node_modules/mocha/bin/_mocha --require test-setup.js tests/*.js",
    "coverage": "babel-node node_modules/isparta/bin/isparta cover  --include-all-sources --report html node_modules/mocha/bin/_mocha -- tests/*.js",
    "build-test": "npm run build && npm run test && npm run coverage"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "chai": "^3.5.0",
    "isparta": "^4.0.0",
    "isparta-instrumenter-loader": "^1.0.1",
    "istanbul": "^0.4.4",
    "jasmine": "^2.4.1",
    "jsdom": "^9.4.1",
    "mocha": "^2.5.3",
    "react-addons-test-utils": "^15.2.1",
    "webpack": "^1.13.1"
  }
}
